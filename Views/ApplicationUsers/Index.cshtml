@model IEnumerable<AuctionProject.Models.ApplicationUser>
<!DOCTYPE html>
<html>
<head>
    <title>User List</title>
</head>
<body>
    <h1>User List</h1>
    <table>
        <thead>
            <tr>
                <th>Email</th>
                <th>Money</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var user in Model)
            {
                <tr>
                    <td>@user.Email</td>
                    <td>@user.Money</td>
                    <td>
                        <a asp-action="Edit" asp-route-email="@user.Email">Edit</a> |
                    </td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>